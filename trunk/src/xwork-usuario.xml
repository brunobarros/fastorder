<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
	
	<package name="usuario" extends="default" namespace="/usuario">
		
		<action name="main">
			<interceptor-ref name="permissionStack" />
            <result type="dispatcher" >main.jsp</result>
        </action>
        
		<action name="login">
            <result type="dispatcher" >login.jsp</result>
        </action>
        
        <action name="entrar" method="login" class="usuarioAction">
	        <interceptor-ref name="publicStack" />
        	<result name="success" type="dispatcher">main.jsp</result>
        	<result name="input" type="dispatcher">login.jsp</result>
        	<result name="error" type="dispatcher">login.jsp</result>        	        	
        </action>
        
        <action name="sair" method="logoff" class="usuarioAction">
        	<result name="success" type="dispatcher">login.jsp</result>        	        	
        </action>
		
	</package>
	
</xwork>